{
  "name": "monocle-decorators",
  "version": "2.0.1",
  "description": "Classy decorators",
  "main": "src/index.js",
  "files": [
    "src/",
    "test/"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.5.2",
    "coveralls": "^2.13.1",
    "jest": "^20.0.4",
    "standard": "^9.0.2",
    "webpack": "^3.0.0"
  },
  "scripts": {
    "test": "jest && npm run lint",
    "lint": "standard --fix --parser babel-eslint 'src/**/*.js' 'test/**/*.js'",
    "preversion": "npm run test",
    "build": "webpack --display-optimization-bailout -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caiogondim/monocle-decorators.js.git"
  },
  "keywords": [
    "decorators",
    "es7",
    "ecmascript7",
    "bind",
    "debounce",
    "freeze",
    "mixin",
    "seal",
    "throttle"
  ],
  "author": "Caio Gondim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caiogondim/monocle-decorators.js/issues"
  },
  "homepage": "https://github.com/caiogondim/monocle-decorators.js#readme",
  "jest": {
    "roots": [
      "test/"
    ],
    "testRegex": "test/.*\\.js$",
    "collectCoverage": true,
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
